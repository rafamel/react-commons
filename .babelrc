{
  "presets": [
    "env",
    "react"
  ],
  "plugins": [
    "transform-decorators-legacy",
    [
      "emotion",
      {
        "autoLabel": true,
        "hoist": true
      }
    ],
    "transform-class-properties",
    [
      "transform-object-rest-spread",
      {
        "useBuiltIns": true
      }
    ]
  ],
  "ignore": [
    "node_modules"
  ]
}